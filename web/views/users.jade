extends layout

block header
  link(rel='stylesheet' type='text/css' href='/stylesheets/blog.css')
  link(rel='stylesheet' type='text/css' href='/stylesheets/users.css')
  title View All Users

block content
  h1.main-title(align='center') View All Users
  each val in docs
    div.user
      div.fullName
        a(href='/users/#{val.username}') #{val.name}
      div.username #{val.username}
      br
